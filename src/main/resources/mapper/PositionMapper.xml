<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.cyber.authing.mapper.PositionMapper">    <sql id="baseColumns">        id        , code, name, order_num, dept_id, tenant_code, description, status, deleted, creator, create_time, updator, update_time, remark, version    </sql>    <sql id="updateColumns">        <set>            <if test="id != null">                id = #{id},            </if>            <if test="code != null">                code = #{code},            </if>            <if test="name != null">                name = #{name},            </if>            <if test="orderNum != null">                order_num = #{orderNum},            </if>            <if test="deptId != null">                dept_id = #{deptId},            </if>            <if test="tenantCode != null">                tenant_code = #{tenantCode},            </if>            <if test="description != null">                description = #{description},            </if>            <if test="status != null">                status = #{status},            </if>            <if test="deleted != null">                deleted = #{deleted},            </if>            <if test="creator != null">                creator = #{creator},            </if>            <if test="createTime != null">                create_time = #{createTime},            </if>            <if test="updator != null">                updator = #{updator},            </if>            <if test="updateTime != null">                update_time = #{updateTime},            </if>            <if test="remark != null">                remark = #{remark},            </if>            <if test="version != null">                version = #{version},            </if>        </set>    </sql>    <sql id="whereClause">        <trim prefix="WHERE" prefixOverrides="AND">            <if test="id != null">                AND ${columnPrefix}id = #{id}            </if>            <if test="code != null">                AND ${columnPrefix}code = #{code}            </if>            <if test="name != null">                AND ${columnPrefix}name = #{name}            </if>            <if test="orderNum != null">                AND ${columnPrefix}order_num = #{orderNum}            </if>            <if test="deptId != null">                AND ${columnPrefix}dept_id = #{deptId}            </if>            <if test="tenantCode != null">                AND ${columnPrefix}tenant_code = #{tenantCode}            </if>            <if test="description != null">                AND ${columnPrefix}description = #{description}            </if>            <if test="status != null">                AND ${columnPrefix}status = #{status}            </if>            <if test="deleted != null">                AND ${columnPrefix}deleted = #{deleted}            </if>            <if test="creator != null">                AND ${columnPrefix}creator = #{creator}            </if>            <if test="createTime != null">                AND ${columnPrefix}create_time = #{createTime}            </if>            <if test="updator != null">                AND ${columnPrefix}updator = #{updator}            </if>            <if test="updateTime != null">                AND ${columnPrefix}update_time = #{updateTime}            </if>            <if test="remark != null">                AND ${columnPrefix}remark = #{remark}            </if>            <if test="version != null">                AND ${columnPrefix}version = #{version}            </if>        </trim>    </sql>    <insert id="save" parameterType="Position">        insert into position        (<include refid="saveColumn"/>)        values        (<include refid="saveValue"/>)    </insert>    <sql id="saveColumn">        <trim suffixOverrides=",">            <if test="id!= null">id,</if>            <if test="code!= null">code,</if>            <if test="name!= null">name,</if>            <if test="orderNum!= null">order_num,</if>            <if test="deptId!= null">dept_id,</if>            <if test="tenantCode!= null">tenant_code,</if>            <if test="description!= null">description,</if>            <if test="status!= null">status,</if>            <if test="deleted!= null">deleted,</if>            <if test="creator!= null">creator,</if>            <if test="createTime!= null">create_time,</if>            <if test="updator!= null">updator,</if>            <if test="updateTime!= null">update_time,</if>            <if test="remark!= null">remark,</if>            <if test="version!= null">version,</if>        </trim>    </sql>    <sql id="saveValue">        <trim suffixOverrides=",">            <if test="id!= null">#{id},</if>            <if test="code!= null">#{code},</if>            <if test="name!= null">#{name},</if>            <if test="orderNum!= null">#{orderNum},</if>            <if test="deptId!= null">#{deptId},</if>            <if test="tenantCode!= null">#{tenantCode},</if>            <if test="description!= null">#{description},</if>            <if test="status!= null">#{status},</if>            <if test="deleted!= null">#{deleted},</if>            <if test="creator!= null">#{creator},</if>            <if test="createTime!= null">#{createTime},</if>            <if test="updator!= null">#{updator},</if>            <if test="updateTime!= null">#{updateTime},</if>            <if test="remark!= null">#{remark},</if>            <if test="version!= null">#{version},</if>        </trim>    </sql>    <delete id="deleteById" parameterType="Position">        DELETE        FROM position        WHERE id = #{id}          and tenant_code = #{tenantCode}    </delete>    <update id="updateById" parameterType="Position">        update position        <include refid="updateColumns"/>        where id = #{id}        and tenant_code = #{tenantCode}    </update>    <select id="selectOne" resultType="Position" parameterType="Position">        SELECT        <include refid="baseColumns"/>        FROM position        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>    </select>    <select id="selectByIndexCount" parameterType="Position" resultType="int">        SELECT count(1)        FROM position        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>    </select>    <select id="selectByIndex" parameterType="Position" resultType="Position">        SELECT        <include refid="baseColumns"/>        FROM position        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>        ORDER BY #{sortBy} #{sortType}        LIMIT ${offset} , ${limit}    </select></mapper>