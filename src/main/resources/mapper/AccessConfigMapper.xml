<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyber.authing.domain.repository.AccessConfigMapper">
	<sql id="baseColumns">
		 id, access_sys, access_key, private_key, public_key, policy_config, status, deleted, creator, create_time, updator, update_time, remark, version
	</sql>

	<sql id="updateColumns">
		<set>
			  <if test="id != null">	id   = #{id},  </if>  <if test="accessSys != null">	access_sys   = #{accessSys},  </if>  <if test="accessKey != null">	access_key   = #{accessKey},  </if>  <if test="privateKey != null">	private_key   = #{privateKey},  </if>  <if test="publicKey != null">	public_key   = #{publicKey},  </if>  <if test="policyConfig != null">	policy_config   = #{policyConfig},  </if>  <if test="status != null">	status   = #{status},  </if>  <if test="deleted != null">	deleted   = #{deleted},  </if>  <if test="creator != null">	creator   = #{creator},  </if>  <if test="createTime != null">	create_time   = #{createTime},  </if>  <if test="updator != null">	updator   = #{updator},  </if>  <if test="updateTime != null">	update_time   = #{updateTime},  </if>  <if test="remark != null">	remark   = #{remark},  </if>  <if test="version != null">	version   = #{version},  </if>
		</set>
	</sql>

    <sql id="whereClause">
        <trim prefix="WHERE" prefixOverrides="AND">
			  <if test="id != null">	    AND ${columnPrefix}id = #{id}  </if>  <if test="accessSys != null">	    AND ${columnPrefix}access_sys = #{accessSys}  </if>  <if test="accessKey != null">	    AND ${columnPrefix}access_key = #{accessKey}  </if>  <if test="privateKey != null">	    AND ${columnPrefix}private_key = #{privateKey}  </if>  <if test="publicKey != null">	    AND ${columnPrefix}public_key = #{publicKey}  </if>  <if test="policyConfig != null">	    AND ${columnPrefix}policy_config = #{policyConfig}  </if>  <if test="status != null">	    AND ${columnPrefix}status = #{status}  </if>  <if test="deleted != null">	    AND ${columnPrefix}deleted = #{deleted}  </if>  <if test="creator != null">	    AND ${columnPrefix}creator = #{creator}  </if>  <if test="createTime != null">	    AND ${columnPrefix}create_time = #{createTime}  </if>  <if test="updator != null">	    AND ${columnPrefix}updator = #{updator}  </if>  <if test="updateTime != null">	    AND ${columnPrefix}update_time = #{updateTime}  </if>  <if test="remark != null">	    AND ${columnPrefix}remark = #{remark}  </if>
        </trim>
    </sql>

	<insert id="save" parameterType="com.cyber.authing.domain.entity.AccessConfig">
		insert into access_config
		(<include refid="saveColumn"/>)
		values
		(<include refid="saveValue"/>)
	</insert>

	<sql id="saveColumn">
		<trim suffixOverrides=",">
			<if test="id!= null">id,</if><if test="accessSys!= null">access_sys,</if><if test="accessKey!= null">access_key,</if><if test="privateKey!= null">private_key,</if><if test="publicKey!= null">public_key,</if><if test="policyConfig!= null">policy_config,</if><if test="status!= null">status,</if><if test="deleted!= null">deleted,</if><if test="creator!= null">creator,</if><if test="createTime!= null">create_time,</if><if test="updator!= null">updator,</if><if test="updateTime!= null">update_time,</if><if test="remark!= null">remark,</if><if test="version!= null">version,</if>
		</trim>
	</sql>

	<sql id="saveValue">
		<trim suffixOverrides=",">
			<if test="id!= null">#{id},</if><if test="accessSys!= null">#{accessSys},</if><if test="accessKey!= null">#{accessKey},</if><if test="privateKey!= null">#{privateKey},</if><if test="publicKey!= null">#{publicKey},</if><if test="policyConfig!= null">#{policyConfig},</if><if test="status!= null">#{status},</if><if test="deleted!= null">#{deleted},</if><if test="creator!= null">#{creator},</if><if test="createTime!= null">#{createTime},</if><if test="updator!= null">#{updator},</if><if test="updateTime!= null">#{updateTime},</if><if test="remark!= null">#{remark},</if><if test="version!= null">#{version},</if>
		</trim>
	</sql>

	<delete id="deleteById" parameterType="com.cyber.authing.domain.entity.AccessConfig">
		DELETE FROM access_config WHERE id = #{id}
	</delete>

	<update id="updateById" parameterType="com.cyber.authing.domain.entity.AccessConfig">
		update access_config
		<include refid="updateColumns" />
		, version = version + 1
		where id = #{id}
		and version = #{version}
	</update>

	<select id="selectOne" resultType="com.cyber.authing.domain.entity.AccessConfig" parameterType="com.cyber.authing.domain.entity.AccessConfig">
		SELECT
		<include refid="baseColumns" />
		FROM access_config
        <include refid="whereClause">
            <property name="columnPrefix" value=""/>
        </include>
	</select>

	<select id="selectByIndexCount" parameterType="com.cyber.authing.domain.entity.AccessConfig" resultType="int">
		SELECT count(1)
		FROM access_config
        <include refid="whereClause">
            <property name="columnPrefix" value=""/>
        </include>
	</select>

	<select id="selectByIndex" parameterType="com.cyber.authing.domain.entity.AccessConfig" resultType="com.cyber.authing.domain.entity.AccessConfig">
		SELECT
		<include refid="baseColumns" />
		FROM access_config
        <include refid="whereClause">
            <property name="columnPrefix" value=""/>
        </include>
		<if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">
			ORDER BY ${sortBy} ${sortType}
		</if>
		LIMIT #{offset} , #{limit}
	</select>
</mapper>
