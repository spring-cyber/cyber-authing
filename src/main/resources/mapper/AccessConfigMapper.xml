<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.cyber.authing.mapper.AccessConfigMapper">    <sql id="baseColumns">        id        , tenant_code, access_sys, access_key, private_key, public_key, policy_config, status, deleted, creator, create_time, updator, update_time, remark, version    </sql>    <sql id="updateColumns">        <set>            <if test="id != null">                id = #{id},            </if>            <if test="tenantCode != null">                tenant_code = #{tenantCode},            </if>            <if test="accessSys != null">                access_sys = #{accessSys},            </if>            <if test="accessKey != null">                access_key = #{accessKey},            </if>            <if test="privateKey != null">                private_key = #{privateKey},            </if>            <if test="publicKey != null">                public_key = #{publicKey},            </if>            <if test="policyConfig != null">                policy_config = #{policyConfig},            </if>            <if test="status != null">                status = #{status},            </if>            <if test="deleted != null">                deleted = #{deleted},            </if>            <if test="creator != null">                creator = #{creator},            </if>            <if test="createTime != null">                create_time = #{createTime},            </if>            <if test="updator != null">                updator = #{updator},            </if>            <if test="updateTime != null">                update_time = #{updateTime},            </if>            <if test="remark != null">                remark = #{remark},            </if>            <if test="version != null">                version = #{version},            </if>        </set>    </sql>    <sql id="whereClause">        <trim prefix="WHERE" prefixOverrides="AND">            <if test="id != null">                AND ${columnPrefix}id = #{id}            </if>            <if test="tenantCode != null">                AND ${columnPrefix}tenant_code = #{tenantCode}            </if>            <if test="accessSys != null">                AND ${columnPrefix}access_sys = #{accessSys}            </if>            <if test="accessKey != null">                AND ${columnPrefix}access_key = #{accessKey}            </if>            <if test="privateKey != null">                AND ${columnPrefix}private_key = #{privateKey}            </if>            <if test="publicKey != null">                AND ${columnPrefix}public_key = #{publicKey}            </if>            <if test="policyConfig != null">                AND ${columnPrefix}policy_config = #{policyConfig}            </if>            <if test="status != null">                AND ${columnPrefix}status = #{status}            </if>            <if test="deleted != null">                AND ${columnPrefix}deleted = #{deleted}            </if>            <if test="creator != null">                AND ${columnPrefix}creator = #{creator}            </if>            <if test="createTime != null">                AND ${columnPrefix}create_time = #{createTime}            </if>            <if test="updator != null">                AND ${columnPrefix}updator = #{updator}            </if>            <if test="updateTime != null">                AND ${columnPrefix}update_time = #{updateTime}            </if>            <if test="remark != null">                AND ${columnPrefix}remark = #{remark}            </if>            <if test="version != null">                AND ${columnPrefix}version = #{version}            </if>        </trim>    </sql>    <insert id="save" parameterType="AccessConfig">        insert into access_config        (<include refid="saveColumn"/>)        values        (<include refid="saveValue"/>)    </insert>    <sql id="saveColumn">        <trim suffixOverrides=",">            <if test="id!= null">id,</if>            <if test="tenantCode!= null">tenant_code,</if>            <if test="accessSys!= null">access_sys,</if>            <if test="accessKey!= null">access_key,</if>            <if test="privateKey!= null">private_key,</if>            <if test="publicKey!= null">public_key,</if>            <if test="policyConfig!= null">policy_config,</if>            <if test="status!= null">status,</if>            <if test="deleted!= null">deleted,</if>            <if test="creator!= null">creator,</if>            <if test="createTime!= null">create_time,</if>            <if test="updator!= null">updator,</if>            <if test="updateTime!= null">update_time,</if>            <if test="remark!= null">remark,</if>            <if test="version!= null">version,</if>        </trim>    </sql>    <sql id="saveValue">        <trim suffixOverrides=",">            <if test="id!= null">#{id},</if>            <if test="tenantCode!= null">#{tenantCode},</if>            <if test="accessSys!= null">#{accessSys},</if>            <if test="accessKey!= null">#{accessKey},</if>            <if test="privateKey!= null">#{privateKey},</if>            <if test="publicKey!= null">#{publicKey},</if>            <if test="policyConfig!= null">#{policyConfig},</if>            <if test="status!= null">#{status},</if>            <if test="deleted!= null">#{deleted},</if>            <if test="creator!= null">#{creator},</if>            <if test="createTime!= null">#{createTime},</if>            <if test="updator!= null">#{updator},</if>            <if test="updateTime!= null">#{updateTime},</if>            <if test="remark!= null">#{remark},</if>            <if test="version!= null">#{version},</if>        </trim>    </sql>    <delete id="deleteById" parameterType="AccessConfig">        DELETE        FROM access_config        WHERE id = #{id}          and tenant_code = #{tenantCode}    </delete>    <update id="updateById" parameterType="AccessConfig">        update access_config        <include refid="updateColumns"/>        where id = #{id}        and tenant_code = #{tenantCode}    </update>    <select id="selectOne" resultType="AccessConfig" parameterType="AccessConfig">        SELECT        <include refid="baseColumns"/>        FROM access_config        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>    </select>    <select id="selectByIndexCount" parameterType="AccessConfig" resultType="int">        SELECT count(1)        FROM access_config        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>    </select>    <select id="selectByIndex" parameterType="AccessConfig" resultType="AccessConfig">        SELECT        <include refid="baseColumns"/>        FROM access_config        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>        ORDER BY #{sortBy} #{sortType}        LIMIT ${offset} , ${limit}    </select></mapper>